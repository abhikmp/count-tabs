{
    "version": "1.0.1",
    "manifest_version": 2,
    "name": "CountTabs",
    "description":"A simple extension which counts the number of tabs in your current window",
    "browser_action":{
        "default_icon": "icon64.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts":["background.js"],
        "persistent":true
    },
    "permissions": [
        "activeTab",
        "storage",
        "tabs",
        "management"
    ]
    
}